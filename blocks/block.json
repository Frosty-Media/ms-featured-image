{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frosty-media/ms-featured-image",
  "version": "0.1.0",
  "title": "Multisite Featured Image",
  "category": "widgets",
  "icon": "multisite",
  "description": "",
  "example": {},
  "attributes": {
    "columns": {
